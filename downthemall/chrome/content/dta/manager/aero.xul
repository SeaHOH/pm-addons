<?xml-stylesheet href="chrome://dta-platform/skin/manager-new.css" type="text/css" ?>
<?xml-stylesheet href="chrome://dta-platform/skin/manager-aero.css" type="text/css" ?>

<overlay xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
	<window id="DownThemAll" chromemargin="0,-1,-1,-1" lightweightthemes="true" lightweightthemesfooter="footer">
		<vbox id="titlebar" insertbefore="toolbox">
			<hbox id="titlebar-content">
				<spacer flex="1"/>
				<hbox id="titlebar-buttonbox" pack="end" align="top">
					<toolbarbutton class="titlebar-button" id="titlebar-min" oncommand="window.minimize();"/>
					<toolbarbutton class="titlebar-button" id="titlebar-max" oncommand="(window.windowState == window.STATE_MAXIMIZED) ? window.restore() : window.maximize();"/>
					<toolbarbutton class="titlebar-button" id="titlebar-close" command="cmdClose"/>
				</hbox>
			</hbox>
		</vbox>
	</window>
</overlay>
